{
  "manifest_version": 2,
  "name": "BetterBlack for YouTube",
  "version": "0.1",
  "content_scripts": [{
    "matches": [
      "https://www.youtube.com/*",
      "http://www.youtube.com/*"
    ],
    "css": ["css/font-awesome.css","css/betterblack.css"]
  }],
  "web_accessible_resources": [
    "images/*",
    "fonts/*"
  ],
  "icons": {
    "16": "images/icons/16.png",
    "48": "images/icons/48.png",
    "128": "images/icons/128.png"
  }
}
